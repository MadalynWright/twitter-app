<p id="notice"><%= notice %></p>

<h1>Whatever, this is just a header</h1>

<% @t.each do |x| %>

<div class="panel panel-primary">
	<div class="panel-heading">
		<h3 class="panel-title"><%= x.user.username %></h3>		
	</div>
	<div class="panel-body">
		<%= x.message %>
	</div>
	<div class="panel-footer">
		<%= link_to 'Show', x, class: "btn btn-primary" %>
		<%= link_to 'Edit', edit_tweet_path(x) %>
		<%= link_to 'Destroy', x, method: :delete, data: { confirm: 'Are you sure?'} %>
	</div>
</div>
<% end %>